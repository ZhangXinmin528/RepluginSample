<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_start_app_one"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:text="@string/startAppOne" />

    <Button
        android:id="@+id/btn_start_plugin_app_one_for_result"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:text="@string/startAppOneForResult" />

    <Button
        android:id="@+id/btn_load_fragment_from_app_one"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:text="@string/startAppOneLoadFragment" />

    <Button
        android:id="@+id/btn_start_kotlin_plugin"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:text="@string/startKotlinPlugin" />

    <Button
        android:id="@+id/btn_start_plugin_webview"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:text="@string/startPluginWebview" />

    <Button
        android:id="@+id/btn_install_apk_from_assets"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:text="@string/installPluginFromAssets" />
</LinearLayout>
